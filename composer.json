{
  "name": "stefano/nette-event-logger",
  "description": "Nette, tracy, symfony event logger",
  "license": "BSD-3-Clause",
  "keywords": [
    "nette",
    "symfony",
    "logger",
    "evet",
    "tracy"
  ],
  "support": {
    "source": "https://github.com/bartko-s/nette-event-logger",
    "issues": "https://github.com/bartko-s/nette-event-logger/issues"
  },
  "homepage": "https://github.com/bartko-s/nette-event-logger",
  "authors": [
    {
      "name": "Å tefan Bartko"
    }
  ],
  "require": {
    "php": ">=8.1",
    "contributte/event-dispatcher": "^0.8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Stefano\\NetteEventLogger\\": "src/"
    },
    "classmap": [
      "./src/"
    ]
  },
  "scripts": {
    "cs-check": "php-cs-fixer --version && php-cs-fixer fix -v --diff --dry-run --allow-risky no",
    "cs-fix": "php-cs-fixer fix -v --allow-risky no"
  }
}